            <!--左边内容-->
            <div class="layui-carousel-left" style="height:600px;">
                <img src="~/Content/images/storephoto/6666.jpg" alt="" class="layui-layer-imgbar" style="width:200px;" />
                <!--动态添加，各种打印服务价格-->
                <div id="price_items" style="width:100%;">

                </div>
                <!--送货价格-->
                <div id="diliver_cahrge" style="width:100%;">
                    <span title="送货价格">1</span>
                </div>
                <!--打印店地址-->
                <div id="location" style="width:100%;">
